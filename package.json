{
  "name": "eslint-plugin-libram",
  "version": "0.3.3",
  "description": "Eslint rules for Libram",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "libram"
  ],
  "author": "Patrick Hulin",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn run tsc",
    "clean": "rm -f {lib,dist,tests}/*.js {lib,dist,tests}/*.d.ts {lib,dist,tests}/**/*.js {lib,dist,tests}/**/*.d.ts",
    "docs": "yarn run typedoc",
    "format": "yarn run prettier --write src/**/* examples/**/*",
    "lint": "yarn run eslint lib tests/lib --ext .ts",
    "prepack": "yarn run build && yarn run updateData",
    "test": "mocha tests --recursive --extension ts --require ts-node/register",
    "updateData": "node --loader ts-node/esm update-data.ts",
    "watch": "yarn run tsc --watch"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "data/*.json"
  ],
  "dependencies": {
    "html-entities": "^2.4.0",
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/estree": "^1.0.1",
    "@types/node": "^20.6.0",
    "@types/node-fetch": "^2.5.12",
    "@types/requireindex": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "data-of-loathing": "^1.4.0",
    "eslint": "^8.25.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
