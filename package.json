{
  "name": "eslint-plugin-libram",
  "version": "0.5.2",
  "description": "Eslint rules for Libram",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "libram"
  ],
  "repository": {
    "url": "https://github.com/loathers/eslint-plugin-libram"
  },
  "author": "Patrick Hulin",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn run tsc",
    "format": "yarn run prettier --write .",
    "lint": "yarn run eslint --ext .ts",
    "test": "yarn run updateData && vitest",
    "coverage": "yarn run updateData && vitest run --coverage",
    "updateData": "node --import tsx lib/loadData.js",
    "watch": "yarn run tsc --watch"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "data/*.json"
  ],
  "dependencies": {
    "data-of-loathing": "^2.6.2",
    "entities": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint__js": "^9.14.0",
    "@types/estree": "1.0.8",
    "@types/node": "^25.0.2",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/rule-tester": "^8.50.0",
    "@vitest/coverage-v8": "4.0.15",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "license": "MIT",
  "packageManager": "yarn@4.3.1"
}
